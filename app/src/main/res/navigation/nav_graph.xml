<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/authFragment">

    <fragment
        android:id="@+id/authFragment"
        android:name="it.mirea.ecoctrl.views.fragments.AuthFragment"
        android:label="AuthFragment">
        <action
            android:id="@+id/action_authFragment_to_webFragment"
            app:destination="@id/webFragment" />
    </fragment>
    <fragment
        android:id="@+id/webFragment"
        android:name="ru.realityfamily.partyapp.Presentation.View.WebFragment"
        android:label="WebFragment">
        <action
            android:id="@+id/action_webFragment_to_authFragment"
            app:destination="@id/authFragment"
            app:popUpTo='@id/nav_graph'
            app:popUpToInclusive="true" />
    </fragment>
</navigation>